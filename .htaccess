<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteBase /

  # Redirect from www to non-www
  RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
  RewriteRule ^(.*)$ https://brandswift.in/$1 [L,R=301]

  # Serve 404.html for any non-existent file or directory
   ErrorDocument 404 /404.astro
  
  # SPA handling
  RewriteCond %{REQUEST_FILENAME} !-f
  RewriteCond %{REQUEST_FILENAME} !-d
  RewriteRule . /index.html [L]
</IfModule>
