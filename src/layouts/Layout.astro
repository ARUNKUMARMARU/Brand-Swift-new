---
interface Props {
  title: string;
  description: string;
  keywords: string;
  robots: string;
  canonical: string;
  og_title: string;
  og_description: string;
  og_image: string;
  twitter_card: string;
  twitter_title: string;
  twitter_description: string;
  twitter_image: string;
}

const {
  title,
  description,
  keywords,
  robots,
  canonical,
  og_title,
  og_description,
  og_image,
  twitter_card,
  twitter_title,
  twitter_description,
  twitter_image,
} = Astro.props;

import '../style/global.css';
---

<!doctype html>
<html lang="en">
  <head>
    <!-- Google Tag Manager -->
    <script>
      (function (w, d, s, l, i) {
        w[l] = w[l] || [];
        w[l].push({ 'gtm.start': new Date().getTime(), event: 'gtm.js' });
        var f = d.getElementsByTagName(s)[0],
          j = d.createElement(s),
          dl = l != 'dataLayer' ? '&l=' + l : '';
        j.async = true;
        j.src = 'https://www.googletagmanager.com/gtm.js?id=' + i + dl;
        f.parentNode.insertBefore(j, f);
      })(window, document, 'script', 'dataLayer', 'GTM-WNT93FXR');
    </script>
    <!-- End Google Tag Manager -->

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="language" content="English" />
    <title>{title}</title>
    <meta name="keywords" content={keywords} />
    <meta name="description" content={description} />
    <meta
      name="viewport"
      content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"
    />
    <!-- <link rel="icon" type="image/svg+xml" href="/favicon.svg" /> -->
    <link rel="icon" href="/standard.jpg" type="image/x-icon">
    <link rel="apple-touch-icon" href="/apple.jpg">
    <link rel="icon" href="/android.jpg" sizes="192x192" type="image/jpg">
    <link rel="canonical" href={canonical} />
    <meta name="robots" content={robots} />
    <meta
      property="og:image"
      content="https://brandswift.in/images/bs-logo-new.png"
    />
    <meta property="og:title" content={og_title} />
    <meta property="og:description" content={og_description} />
    <meta property="og:image" content={og_image} />
    <meta property="og:url" content={canonical} />
    <meta property="og:type" content="website" />

    <meta name="twitter:card" content={twitter_card} />
    <meta name="twitter:title" content={twitter_title} />
    <meta name="twitter:description" content={twitter_description} />
    <meta name="twitter:image" content={twitter_image} />
    <meta
      name="google-site-verification"
      content="w96wX-JB_CULrzcFSSiSwLZCxGk8bqoA6ns53RqDNrQ"
    />

    <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-12VV99KQ4D"
    ></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag() {
        dataLayer.push(arguments);
      }
      gtag('js', new Date());

      gtag('config', 'G-12VV99KQ4D');
    </script>

    <meta name="generator" content={Astro.generator} />

    <meta property="og:title" content={title} />
    <meta
      property="og:description"
      content="We are a Top Digital Marketing Agency in Bangalore"
    />

    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css"
      integrity="sha512-tS3S5qG0BlhnQROyJXvNjeEM4UpMXHrQfTGmbQ1gKmelCxlSEBUaxhRBj/EFTzpbP4RVSrpEikbmdJobCvhE3g=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.css"
      integrity="sha512-OTcub78R3msOCtY3Tc6FzeDJ8N9qvQn1Ph49ou13xgA9VsH9+LRxoFU6EqLhW4+PKRfU+/HReXmSZXHEkpYoOA=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link
      href="https://fonts.googleapis.com/css2?family=Prompt:wght@200;300;400;500;600&family=Work+Sans:wght@400;500;600;700;800&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Poppins:wght@200;300;400;500;600;700&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Kanit:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap"
      rel="stylesheet"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/tiny-slider/2.9.4/tiny-slider.css"
    />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC"
      crossorigin="anonymous"
    />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
    />

    <script type="application/ld+json">
      @context": "https://schema.org",
        "@type": ["LocalBusiness", "Organization"],
        "name": "Brand Swift",
        "image": "https://brandswift.in/images/bs-logo-new.png",
        "address": {
          "@type": "PostalAddress",
          "streetAddress": "3, Banashankari 6th Stage, 4th Block",
          "addressLocality": "Bangalore",
          "addressRegion": "KA",
          "postalCode": "560062",
          "addressCountry": "IN"
        },
        "telephone": "+91-855-333-3145",
        "email": "hello@brandswift.in",
        "url": "https://www.brandswift.in",
        "openingHours": "Mo-Fr 09:00-18:00",
        "description": "Brand Swift is a digital marketing agency based in Bangalore, specializing in SEO, SEM, and social media marketing.",
        "sameAs": "https://www.instagram.com/brandswift.in/
    </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/vue/2.6.10/vue.min.js"
    ></script>
  </head>
  <body>
    <!-- Google Tag Manager (noscript) -->
    <noscript
      ><iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-WNT93FXR"
        height="0"
        width="0"
        style="display:none;visibility:hidden"></iframe></noscript
    >
    <!-- End Google Tag Manager (noscript) -->

    <div class="blob-wrapper">
      <div class="blob-section">
        <div class="blob" id="blob1"></div>
        <div class="blob" id="blob2"></div>
        <div class="blob" id="blob4"></div>
      </div>
    </div>
    <div id="follower" class="follow"></div>
    <slot />
    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.3/jquery.min.js"
      integrity="sha512-STof4xm1wgkfm7heWqFJVn58Hm3EtS31XFaagaa8VMReCXAkQnJZ+jEy8PCC/iT18dFy95WcExNHFTqLyp72eQ=="
      crossorigin="anonymous"
      is:inline
      referrerpolicy="no-referrer"></script>

    <script
      src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js"
      integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r"
      crossorigin="anonymous"></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js"
      integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy"
      crossorigin="anonymous"></script>

    <script is:inline src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>

    <script
      src="https://cdn.jsdelivr.net/gh/fancyapps/fancybox@3.5.7/dist/jquery.fancybox.min.js"
    ></script>

    <script
      src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/owl.carousel.min.js"
      integrity="sha512-bPs7Ae6pVvhOSiIcyUClR7/q2OAsRiovw4vAkX+zJbw3ShAeeqezq50RIIcIURq7Oa20rW2n2q+fyXBNcU9lrw=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"></script>

    <script>
      import '../app';
      import '../homeanimation';
    </script>

    <script>
      var follower = document.getElementById('follower');
      var hoveredDiv = document.querySelector('.overlay');

      document.addEventListener('mousemove', function (e) {
        var xPos = e.clientX - follower.offsetWidth / 2;
        var yPos = e.clientY - follower.offsetHeight / 2;
        follower.style.left = xPos + 'px';
        follower.style.top = yPos + 'px';
      });
    </script>

    <script
      type="text/javascript"
      src="https://cdn.jsdelivr.net/npm/emailjs-com@3/dist/email.min.js"
    ></script>
    <script type="text/javascript">
      (function () {
        emailjs.init('cm5iGQ0mNf7Rsv9K0');
      })();
    </script>
  </body>
</html>
